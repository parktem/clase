package Ejercicio02;

import java.applet.Applet;
import java.awt.Image;

public class Puzzle extends Applet {

	Image[] imagenes;

	public void init() {
		imagenes = new Image[25];
		for (int i = 0; i < imagenes.length; i++) {
			imagenes[i] = getImage(getCodeBase(), "directorioImagenes" + (i + 1) + ".png");
		}
	}

}
